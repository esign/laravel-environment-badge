{
    "name": "esign/laravel-environment-badge",
    "description": "Display a warning badge for certain app environments",
    "keywords": [
        "esign",
        "environment-badge"
    ],
    "homepage": "https://github.com/esign/laravel-environment-badge",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jordy Van der Haegen",
            "email": "jordy.vanderhaegen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/view": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Esign\\EnvironmentBadge\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esign\\EnvironmentBadge\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esign\\EnvironmentBadge\\EnvironmentBadgeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
